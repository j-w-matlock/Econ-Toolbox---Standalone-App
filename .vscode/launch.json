{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Launch EconToolbox.Desktop",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "dotnet",
      "args": [
        "run",
        "--project",
        "EconToolbox.Desktop.csproj"
      ],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
    }
  ]
}
