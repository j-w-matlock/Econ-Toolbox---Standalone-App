<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Core colors -->
    <Color x:Key="App.Background.Color">#F3F3F3</Color>
    <Color x:Key="App.Surface.Color">#FFFFFF</Color>
    <Color x:Key="App.SurfaceAlt.Color">#F3F3F3</Color>
    <Color x:Key="App.Surface2.Color">#F8F8F8</Color>
    <Color x:Key="App.Border.Color">#D0D0D0</Color>
    <Color x:Key="App.Divider.Color">#D0D0D0</Color>
    <Color x:Key="App.TextPrimary.Color">#1E1E1E</Color>
    <Color x:Key="App.TextSecondary.Color">#5A5A5A</Color>
    <Color x:Key="App.TextDisabled.Color">#9A9A9A</Color>
    <Color x:Key="App.Accent.Color">#0E639C</Color>
    <Color x:Key="App.AccentHover.Color">#007ACC</Color>
    <Color x:Key="App.AccentPressed.Color">#094771</Color>
    <Color x:Key="App.OnAccent.Color">#FFFFFF</Color>
    <Color x:Key="App.ControlBackground.Color">#FFFFFF</Color>
    <Color x:Key="App.ControlHover.Color">#F3F3F3</Color>
    <Color x:Key="App.ControlPressed.Color">#E5E5E5</Color>
    <Color x:Key="App.Validation.Color">#D13438</Color>
    <Color x:Key="App.Warning.Color">#CCA700</Color>
    <Color x:Key="App.Success.Color">#107C10</Color>
    <Color x:Key="App.Focus.Color">#007ACC</Color>

    <!-- Shell colors -->
    <Color x:Key="App.NavigationBackground.Color">#F3F3F3</Color>
    <Color x:Key="App.NavigationSelectedBackground.Color">#E5E5E5</Color>
    <Color x:Key="App.NavigationBorder.Color">#D0D0D0</Color>
    <Color x:Key="App.NavigationText.Color">#1E1E1E</Color>
    <Color x:Key="App.HighlightBackground.Color">#E5F1FB</Color>
    <Color x:Key="App.HighlightBorder.Color">#9CC7F7</Color>
    <Color x:Key="App.TitleBarBackground.Color">#E7E7E7</Color>
    <Color x:Key="App.StatusBarBackground.Color">#E7E7E7</Color>
    <Color x:Key="App.EditorBackground.Color">#FFFFFF</Color>

    <!-- Feedback colors -->
    <Color x:Key="App.WarningBackground.Color">#FFF4CE</Color>
    <Color x:Key="App.WarningBorder.Color">#CCA700</Color>
    <Color x:Key="App.AlertForeground.Color">#A1260D</Color>

    <!-- Tooltip/Menu colors -->
    <Color x:Key="App.ToolTipBackground.Color">#0E639C</Color>
    <Color x:Key="App.ToolTipBorder.Color">#1C4E68</Color>
    <Color x:Key="App.ToolTipForeground.Color">#FFFFFF</Color>
    <Color x:Key="App.MenuBackground.Color">#FFFFFF</Color>
    <Color x:Key="App.MenuBorder.Color">#D0D0D0</Color>
    <Color x:Key="App.MenuSelectionBackground.Color">#E5F1FB</Color>
    <Color x:Key="App.MenuSelectionForeground.Color">#1E1E1E</Color>

    <!-- Charts/selection -->
    <Color x:Key="App.ChartGrid.Color">#D0D0D0</Color>
    <Color x:Key="App.ChartFill.Color">#FFFFFF</Color>
    <Color x:Key="App.Chart.Series1.Color">#2D6A8E</Color>
    <Color x:Key="App.Chart.Series2.Color">#1ABC9C</Color>
    <Color x:Key="App.Chart.Series3.Color">#7F56D9</Color>
    <Color x:Key="App.Chart.Series4.Color">#F39C12</Color>
    <Color x:Key="App.Chart.Series5.Color">#3498DB</Color>
    <Color x:Key="App.Chart.Series6.Color">#E74C3C</Color>
    <Color x:Key="App.SelectionBackground.Color">#D1F0EA</Color>
    <Color x:Key="App.SelectionBorder.Color">#00A189</Color>
    <Color x:Key="App.SelectionHoverBackground.Color">#E3F7F2</Color>
    <Color x:Key="App.RowHoverBackground.Color">#E3F7F2</Color>

    <!-- Scrollbars -->
    <Color x:Key="App.ScrollBarThumb.Color">#B0B0B0</Color>
    <Color x:Key="App.ScrollBarThumbHover.Color">#909090</Color>
    <Color x:Key="App.ScrollBarTrack.Color">#E5E5E5</Color>

    <!-- Hero/shadow -->
    <Color x:Key="App.HeroStart.Color">#2C2C2C</Color>
    <Color x:Key="App.HeroEnd.Color">#1E1E1E</Color>
    <Color x:Key="App.HeroBorder.Color">#1AFFFFFF</Color>
    <Color x:Key="App.Shadow.Color">#33000000</Color>

    <!-- Diagnostics -->
    <Color x:Key="App.Diagnostic.InfoBackground.Color">#E8F2FF</Color>
    <Color x:Key="App.Diagnostic.InfoBorder.Color">#C3D9F3</Color>
    <Color x:Key="App.Diagnostic.InfoIcon.Color">#007ACC</Color>
    <Color x:Key="App.Diagnostic.WarningBackground.Color">#FFF4CE</Color>
    <Color x:Key="App.Diagnostic.WarningBorder.Color">#CCA700</Color>
    <Color x:Key="App.Diagnostic.WarningIcon.Color">#CCA700</Color>
    <Color x:Key="App.Diagnostic.AdvisoryBackground.Color">#FFE8D2</Color>
    <Color x:Key="App.Diagnostic.AdvisoryBorder.Color">#D17A22</Color>
    <Color x:Key="App.Diagnostic.AdvisoryIcon.Color">#D17A22</Color>
    <Color x:Key="App.Diagnostic.ErrorBackground.Color">#FDE7E9</Color>
    <Color x:Key="App.Diagnostic.ErrorBorder.Color">#D13438</Color>
    <Color x:Key="App.Diagnostic.ErrorIcon.Color">#D13438</Color>

    <!-- Link colors -->
    <Color x:Key="App.Link.JointUse.Color">#C54F1F</Color>
    <Color x:Key="App.Link.PreEnr.Color">#9B3D8A</Color>
    <Color x:Key="App.Link.TransitionEnr.Color">#0F7C8B</Color>
    <Color x:Key="App.Link.Enr1967.Color">#5A3CA0</Color>
    <Color x:Key="App.Link.CwccisBase.Color">#2E6EB5</Color>
    <Color x:Key="App.Link.CwccisCurrent.Color">#5E8D2A</Color>

    <!-- Brushes -->
    <SolidColorBrush x:Key="App.Background" Color="{DynamicResource App.Background.Color}"/>
    <SolidColorBrush x:Key="App.Surface" Color="{DynamicResource App.Surface.Color}"/>
    <SolidColorBrush x:Key="App.SurfaceAlt" Color="{DynamicResource App.SurfaceAlt.Color}"/>
    <SolidColorBrush x:Key="App.Surface2" Color="{DynamicResource App.Surface2.Color}"/>
    <SolidColorBrush x:Key="App.Border" Color="{DynamicResource App.Border.Color}"/>
    <SolidColorBrush x:Key="App.Divider" Color="{DynamicResource App.Divider.Color}"/>
    <SolidColorBrush x:Key="App.TextPrimary" Color="{DynamicResource App.TextPrimary.Color}"/>
    <SolidColorBrush x:Key="App.TextSecondary" Color="{DynamicResource App.TextSecondary.Color}"/>
    <SolidColorBrush x:Key="App.TextDisabled" Color="{DynamicResource App.TextDisabled.Color}"/>
    <SolidColorBrush x:Key="App.Accent" Color="{DynamicResource App.Accent.Color}"/>
    <SolidColorBrush x:Key="App.AccentHover" Color="{DynamicResource App.AccentHover.Color}"/>
    <SolidColorBrush x:Key="App.AccentPressed" Color="{DynamicResource App.AccentPressed.Color}"/>
    <SolidColorBrush x:Key="App.OnAccent" Color="{DynamicResource App.OnAccent.Color}"/>
    <SolidColorBrush x:Key="App.ControlBackground" Color="{DynamicResource App.ControlBackground.Color}"/>
    <SolidColorBrush x:Key="App.ControlHover" Color="{DynamicResource App.ControlHover.Color}"/>
    <SolidColorBrush x:Key="App.ControlPressed" Color="{DynamicResource App.ControlPressed.Color}"/>
    <SolidColorBrush x:Key="App.Validation" Color="{DynamicResource App.Validation.Color}"/>
    <SolidColorBrush x:Key="App.Warning" Color="{DynamicResource App.Warning.Color}"/>
    <SolidColorBrush x:Key="App.Success" Color="{DynamicResource App.Success.Color}"/>
    <SolidColorBrush x:Key="App.Focus" Color="{DynamicResource App.Focus.Color}"/>

    <SolidColorBrush x:Key="App.NavigationBackground" Color="{DynamicResource App.NavigationBackground.Color}"/>
    <SolidColorBrush x:Key="App.NavigationSelectedBackground" Color="{DynamicResource App.NavigationSelectedBackground.Color}"/>
    <SolidColorBrush x:Key="App.NavigationBorder" Color="{DynamicResource App.NavigationBorder.Color}"/>
    <SolidColorBrush x:Key="App.NavigationText" Color="{DynamicResource App.NavigationText.Color}"/>
    <SolidColorBrush x:Key="App.HighlightBackground" Color="{DynamicResource App.HighlightBackground.Color}"/>
    <SolidColorBrush x:Key="App.HighlightBorder" Color="{DynamicResource App.HighlightBorder.Color}"/>
    <SolidColorBrush x:Key="App.TitleBarBackground" Color="{DynamicResource App.TitleBarBackground.Color}"/>
    <SolidColorBrush x:Key="App.StatusBarBackground" Color="{DynamicResource App.StatusBarBackground.Color}"/>
    <SolidColorBrush x:Key="App.EditorBackground" Color="{DynamicResource App.EditorBackground.Color}"/>

    <SolidColorBrush x:Key="App.WarningBackground" Color="{DynamicResource App.WarningBackground.Color}"/>
    <SolidColorBrush x:Key="App.WarningBorder" Color="{DynamicResource App.WarningBorder.Color}"/>
    <SolidColorBrush x:Key="App.AlertForeground" Color="{DynamicResource App.AlertForeground.Color}"/>

    <SolidColorBrush x:Key="App.ToolTipBackground" Color="{DynamicResource App.ToolTipBackground.Color}"/>
    <SolidColorBrush x:Key="App.ToolTipBorder" Color="{DynamicResource App.ToolTipBorder.Color}"/>
    <SolidColorBrush x:Key="App.ToolTipForeground" Color="{DynamicResource App.ToolTipForeground.Color}"/>
    <SolidColorBrush x:Key="App.MenuBackground" Color="{DynamicResource App.MenuBackground.Color}"/>
    <SolidColorBrush x:Key="App.MenuBorder" Color="{DynamicResource App.MenuBorder.Color}"/>
    <SolidColorBrush x:Key="App.MenuSelectionBackground" Color="{DynamicResource App.MenuSelectionBackground.Color}"/>
    <SolidColorBrush x:Key="App.MenuSelectionForeground" Color="{DynamicResource App.MenuSelectionForeground.Color}"/>

    <SolidColorBrush x:Key="App.ChartGrid" Color="{DynamicResource App.ChartGrid.Color}"/>
    <SolidColorBrush x:Key="App.ChartFill" Color="{DynamicResource App.ChartFill.Color}"/>
    <SolidColorBrush x:Key="App.Chart.Series1" Color="{DynamicResource App.Chart.Series1.Color}"/>
    <SolidColorBrush x:Key="App.Chart.Series2" Color="{DynamicResource App.Chart.Series2.Color}"/>
    <SolidColorBrush x:Key="App.Chart.Series3" Color="{DynamicResource App.Chart.Series3.Color}"/>
    <SolidColorBrush x:Key="App.Chart.Series4" Color="{DynamicResource App.Chart.Series4.Color}"/>
    <SolidColorBrush x:Key="App.Chart.Series5" Color="{DynamicResource App.Chart.Series5.Color}"/>
    <SolidColorBrush x:Key="App.Chart.Series6" Color="{DynamicResource App.Chart.Series6.Color}"/>
    <SolidColorBrush x:Key="App.SelectionBackground" Color="{DynamicResource App.SelectionBackground.Color}"/>
    <SolidColorBrush x:Key="App.SelectionBorder" Color="{DynamicResource App.SelectionBorder.Color}"/>
    <SolidColorBrush x:Key="App.SelectionHoverBackground" Color="{DynamicResource App.SelectionHoverBackground.Color}"/>
    <SolidColorBrush x:Key="App.RowHoverBackground" Color="{DynamicResource App.RowHoverBackground.Color}"/>

    <SolidColorBrush x:Key="App.ScrollBarThumb" Color="{DynamicResource App.ScrollBarThumb.Color}"/>
    <SolidColorBrush x:Key="App.ScrollBarThumbHover" Color="{DynamicResource App.ScrollBarThumbHover.Color}"/>
    <SolidColorBrush x:Key="App.ScrollBarTrack" Color="{DynamicResource App.ScrollBarTrack.Color}"/>

    <SolidColorBrush x:Key="App.HeroBorder" Color="{DynamicResource App.HeroBorder.Color}"/>

    <SolidColorBrush x:Key="App.Diagnostic.InfoBackground" Color="{DynamicResource App.Diagnostic.InfoBackground.Color}"/>
    <SolidColorBrush x:Key="App.Diagnostic.InfoBorder" Color="{DynamicResource App.Diagnostic.InfoBorder.Color}"/>
    <SolidColorBrush x:Key="App.Diagnostic.InfoIcon" Color="{DynamicResource App.Diagnostic.InfoIcon.Color}"/>
    <SolidColorBrush x:Key="App.Diagnostic.WarningBackground" Color="{DynamicResource App.Diagnostic.WarningBackground.Color}"/>
    <SolidColorBrush x:Key="App.Diagnostic.WarningBorder" Color="{DynamicResource App.Diagnostic.WarningBorder.Color}"/>
    <SolidColorBrush x:Key="App.Diagnostic.WarningIcon" Color="{DynamicResource App.Diagnostic.WarningIcon.Color}"/>
    <SolidColorBrush x:Key="App.Diagnostic.AdvisoryBackground" Color="{DynamicResource App.Diagnostic.AdvisoryBackground.Color}"/>
    <SolidColorBrush x:Key="App.Diagnostic.AdvisoryBorder" Color="{DynamicResource App.Diagnostic.AdvisoryBorder.Color}"/>
    <SolidColorBrush x:Key="App.Diagnostic.AdvisoryIcon" Color="{DynamicResource App.Diagnostic.AdvisoryIcon.Color}"/>
    <SolidColorBrush x:Key="App.Diagnostic.ErrorBackground" Color="{DynamicResource App.Diagnostic.ErrorBackground.Color}"/>
    <SolidColorBrush x:Key="App.Diagnostic.ErrorBorder" Color="{DynamicResource App.Diagnostic.ErrorBorder.Color}"/>
    <SolidColorBrush x:Key="App.Diagnostic.ErrorIcon" Color="{DynamicResource App.Diagnostic.ErrorIcon.Color}"/>

    <SolidColorBrush x:Key="App.Link.JointUse" Color="{DynamicResource App.Link.JointUse.Color}"/>
    <SolidColorBrush x:Key="App.Link.PreEnr" Color="{DynamicResource App.Link.PreEnr.Color}"/>
    <SolidColorBrush x:Key="App.Link.TransitionEnr" Color="{DynamicResource App.Link.TransitionEnr.Color}"/>
    <SolidColorBrush x:Key="App.Link.Enr1967" Color="{DynamicResource App.Link.Enr1967.Color}"/>
    <SolidColorBrush x:Key="App.Link.CwccisBase" Color="{DynamicResource App.Link.CwccisBase.Color}"/>
    <SolidColorBrush x:Key="App.Link.CwccisCurrent" Color="{DynamicResource App.Link.CwccisCurrent.Color}"/>

    <LinearGradientBrush x:Key="App.HeroBackground" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="{DynamicResource App.HeroStart.Color}" Offset="0"/>
        <GradientStop Color="{DynamicResource App.HeroEnd.Color}" Offset="1"/>
    </LinearGradientBrush>
</ResourceDictionary>
